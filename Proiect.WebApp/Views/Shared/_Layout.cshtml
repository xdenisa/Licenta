<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js" type="text/javascript"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/css/mycss.css" type="text/css" />
    <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
    <link rel="icon" href="/images/logo.png" type="image/x-icon">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbarcustom">
            <div class="container-fluid">
                <div class="float-left">
                    <a href="/"><img id="imagineLogo" src="~/images/logo.png" /></a>
                    <label class="navitems">Medicify</label>
                </div>
                <div class="collapse d-inline-block float-right" id="navbarCollpase">
                    <ul class="navbar-nav ">
                        <li class="nav-item"><a class="nav-link navitems" asp-controller="Home" asp-action="Index">Acasă</a></li>
                        <li class="nav-item"><a class="nav-link navitems" asp-controller="Contact" asp-action="Index">Contact</a></li>
                        <partial name="_LoginPartial" />
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>
    @RenderSection("Scripts", required: false)
    @RenderSection("Styles", false)
    @*<footer>
    </footer>*@
</body>
</html>
